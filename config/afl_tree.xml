            <Sequence name="Liftup">
                <MoveToHeight name="MoveToHeight" message="{targetPose}"/>
                <MoveForward name="MoveForward" message="{targetPose}"/>
                <ForkHigher  name="ForkHigher" message="{targetPose}"/>
                <MoveBackward name="MoveBackward" message="{targetPose}"/>
                <DropFork name="DropFork" Height="250"/>
            </Sequence>
