 <root main_tree_to_execute = "AFL_BT" >
     <BehaviorTree ID="AFL_BT">
        <Sequence name="root_sequence">
            <MoveToReadyPoint   name="MoveToReadyPoint" ReadyPoint="0.3;2.58;0"/> <!-- ReadyPoint:x,y,yaw -->
            <!-- <NormalDetection name="Start Detection"/> -->
            <MoveToGoal   name="MoveToGoal" Pose="{targetPose}"/>
